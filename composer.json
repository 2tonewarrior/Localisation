{
    "name": "laravel-enso/localisation",
    "description": "Localisation management dependency for Laravel Enso",
    "keywords": [
        "laravel-enso",
        "localisation"
    ],
    "homepage": "https://github.com/laravel-enso/Localisation",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-enso.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.1.0",
        "laravel/framework": "5.5.*",
        "laravel-enso/core": "1.3.*",
        "laravel-enso/datatable": "1.3.*",
        "laravel-enso/formbuilder": "1.3.*",
        "laravel-enso/helpers": "1.3.*",
        "laravel-enso/structuremanager": "1.3.*",
        "jenssegers/date": "^3.2"
    },
    "require-dev": {
        "laravel-enso/testhelper": "1.3.*"
    },
    "autoload": {
        "psr-4": {
            "LaravelEnso\\Localisation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelEnso\\Localisation\\LocalisationServiceProvider"
            ],
            "aliases": {}
        }
    }
}
